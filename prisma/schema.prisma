generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Profile {
  id           String   @id @default(uuid())
  firstName    String
  lastName     String
  email        String   @unique
  profileImage String
  job          String
  nickname     String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
